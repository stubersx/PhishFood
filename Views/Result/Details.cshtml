@model PhishFood.Models.Result

@{
    ViewData["Title"] = "Result Details";
}

<h1 class="text-center mt-5">@ViewData["Title"]</h1>

<hr />
<dl class="row my-5 max100">
    <dt class="offset-md-4 col-md-2">
        @Html.DisplayNameFor(model => model.Date):
    </dt>
    <dd class="col-md-6">
        @Html.DisplayFor(model => model.Date)
    </dd>
    <dt class="offset-md-4 col-md-2">
        @Html.DisplayNameFor(model => model.Score):
    </dt>
    <dd class="col-md-6">
        @Html.DisplayFor(model => model.Score)
    </dd>
    <dt class="offset-md-4 col-md-2">
        @Html.DisplayNameFor(model => model.Category):
    </dt>
    <dd class="col-md-6">
        @Html.DisplayFor(model => model.Category.Type)
    </dd>
    <dt class="offset-md-4 col-md-2">
        @Html.DisplayNameFor(model => model.Student.ID):
    </dt>
    <dd class="col-md-6">
        @Html.DisplayFor(model => model.Student.ID)
    </dd>
</dl>
<div class="form-group btns mb-5">
    <a asp-action="Edit" asp-route-id="@Model?.ID" class="btn btn-outline-danger">Edit</a>
    <a asp-action="Index" class="btn btn-outline-dark">Back to List</a>
</div>
